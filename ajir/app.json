{
  "expo": {
    "name": "أجر",
    "slug": "ajir",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/logo.png",
    "scheme": "ajir",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "userInterfaceStyle": "automatic",
      "backgroundColor": "#0F172A",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/logo.png",
        "backgroundColor": "#0F172A",
        "backgroundImage": "./src/assets/images/logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.ajir"
    },
    "web": {
      "output": "static",
      "favicon": "./src/assets/images/logo.png"
    },
    "plugins": ["expo-router", "expo-font", "react-native-edge-to-edge"],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "fbc46d7e-9ced-4bdb-b411-5c9962ff4014"
      }
    },
    "owner": "abdullahseraf"
  }
}
