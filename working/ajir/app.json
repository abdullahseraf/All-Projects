{
  "expo": {
    "name": "أجر",
    "slug": "ajir",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/Group 8-1.png",
    "scheme": "ajir",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "userInterfaceStyle": "automatic",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/Group 8-1.png",
        "backgroundImage": "./src/assets/images/Group 8-1.png",
        "monochromeImage": "./src/assets/images/Group 8-1.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.ajir"
    },
    "web": {
      "output": "static",
      "favicon": "./src/assets/images/Group 8-1.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/Group 8-1.png",
          "imageWidth": 300,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "0F172A"
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "fbc46d7e-9ced-4bdb-b411-5c9962ff4014"
      }
    },
    "owner": "abdullahseraf"
  }
}
