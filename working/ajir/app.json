{
  "expo": {
    "name": "اجر",
    "slug": "ajir",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/Group 8-1.png",
    "scheme": "ajir",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./src/assets/images/Group 8-1.png",
        "backgroundImage": "./src/assets/images/Group 8-1.png",
        "monochromeImage": "./src/assets/images/Group 8-1.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.ajir"
    },
    "web": {
      "output": "static",
      "favicon": "./src/assets/images/Group 8-1.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/Group 8-1.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
